def queen_move_simple():
    """
    Программа для проверки хода ферзя
    """
    
    try:
        # Ввод координат
        print("Введите координаты двух клеток:")
        x1 = int(input("Столбец первой клетки: "))
        y1 = int(input("Строка первой клетки: "))
        x2 = int(input("Столбец второй клетки: "))
        y2 = int(input("Строка второй клетки: "))
        
        # Проверка что координаты от 1 до 8
        if x1 < 1 or x1 > 8 or y1 < 1 or y1 > 8 or x2 < 1 or x2 > 8 or y2 < 1 or y2 > 8:
            print("Ошибка: координаты должны быть от 1 до 8")
            return
        
        # Проверка что клетки разные
        if x1 == x2 and y1 == y2:
            print("NO")
            return
        
        # Проверка может ли ферзь сделать ход
        # Ферзь ходит по горизонтали, вертикали или диагонали
        if x1 == x2:        # Вертикальный ход
            print("YES")
        elif y1 == y2:      # Горизонтальный ход  
            print("YES")
        elif abs(x1 - x2) == abs(y1 - y2):  # Диагональный ход
            print("YES")
        else:
            print("NO")
            
    except:
        print("Ошибка ввода")

# Запуск программы
queen_move_simple()
